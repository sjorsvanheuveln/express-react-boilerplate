{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": ["error", "unix"],
        "import/newline-after-import": 0,
        "react/react-in-jsx-scope" : 0, //react doesn't have to be in scope anymore
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "react/jsx-one-expression-per-line": 0,
        "react/prop-types": 0,
        "object-curly-newline": 0
    }
}
